<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="group"
                android:pivotX="12"
                android:pivotY="12">
                <path
                    android:name="path"
                    android:fillColor="?attr/colorOnSurface"
                    android:pathData="M 5 6 L 19 6 C 19.55 6 20 5.55 20 5 C 20 4.45 19.55 4 19 4 L 5 4 C 4.45 4 4 4.45 4 5 C 4 5.55 4.45 6 5 6 Z M 20.16 7.8 C 20.07 7.34 19.66 7 19.18 7 L 4.82 7 C 4.34 7 3.93 7.34 3.84 7.8 L 2.84 12.8 C 2.72 13.42 3.19 14 3.82 14 L 4 14 L 4 19 C 4 19.55 4.45 20 5 20 L 13 20 C 13.55 20 14 19.55 14 19 L 14 14 L 18 14 L 18 19 C 18 19.55 18.45 20 19 20 C 19.55 20 20 19.55 20 19 L 20 14 L 20.18 14 C 20.81 14 21.28 13.42 21.16 12.8 L 20.16 7.8 Z M 12 18 L 6 18 L 6 14 L 12 14 L 12 18 Z" />
            </group>
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="0"
                android:interpolator="@android:interpolator/fast_out_slow_in"
                android:propertyName="fillColor"
                android:valueFrom="?attr/colorOnSurface"
                android:valueTo="?attr/colorError"
                android:valueType="colorType" />
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="scaleX"
                    android:valueFrom="1"
                    android:valueTo="1.35"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="scaleY"
                    android:valueFrom="1"
                    android:valueTo="1.35"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="scaleX"
                    android:startOffset="250"
                    android:valueFrom="1.35"
                    android:valueTo="1"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="scaleY"
                    android:startOffset="250"
                    android:valueFrom="1.35"
                    android:valueTo="1"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="scaleX"
                    android:startOffset="500"
                    android:valueFrom="1"
                    android:valueTo="1.35"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="scaleY"
                    android:startOffset="500"
                    android:valueFrom="1"
                    android:valueTo="1.35"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="scaleX"
                    android:startOffset="750"
                    android:valueFrom="1.35"
                    android:valueTo="1"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="scaleY"
                    android:startOffset="750"
                    android:valueFrom="1.35"
                    android:valueTo="1"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
</animated-vector>
