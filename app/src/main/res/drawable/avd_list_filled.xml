<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="path"
                android:pathData="M 12 23 L 17 23 L 17 20 L 20 20 L 20 16 L 23 16 L 23 8 L 20 8 L 20 4 L 17 4 L 17 1 L 7 1 L 7 4 L 4 4 L 4 8 L 1 8 L 1 16 L 4 16 L 4 20 L 7 20 L 7 23 Z"
                android:strokeWidth="1"
                android:strokeColor="?emptyHintColor"
                android:strokeLineCap="round"
                android:strokeLineJoin="round" />
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="300"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0"
                    android:valueTo="0.5"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="300"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="trimPathEnd"
                    android:valueFrom="1"
                    android:valueTo="0.5"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
</animated-vector>
